{
  "manifest_version": 2,
  "name": "Rinse browser data",
  "version": "0.1",
  "icons": {
    "48": "icon_dark.svg",
    "96": "icon_dark.svg"
  },
  "browser_action": {
    "default_title": "Rinse browser data",
    "default_popup": "public/index.html",
    "default_icon": "icon_light.svg",
    "theme_icons": [
      {
        "light": "icon_light.svg",
        "dark": "icon_dark.svg",
        "size": 32
      }
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "browsingData",
    "<all_urls>",
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "storage"
  ],
  "default_locale": "en"
}